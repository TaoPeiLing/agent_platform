{
    "app_name": "SSS Agent Platform - 企业连接器",
    "version": "1.0.0",
    "build_year": "2023",
    "theme": {
        "primary_color": "#FF4B4B",
        "background_color": "#FFFFFF",
        "secondary_background_color": "#F0F2F6",
        "text_color": "#262730",
        "font": "sans-serif"
    },
    "connectors": {
